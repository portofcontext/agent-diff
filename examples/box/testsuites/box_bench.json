{
    "id": "box-bench",
    "name": "Box Bench",
    "description": "Benchmark tests for Box MCP operations",
    "service": "box",
    "ignore_fields": {
        "global": [
            "created_at",
            "modified_at",
            "content_created_at",
            "content_modified_at",
            "purged_at",
            "trashed_at",
            "etag",
            "sequence_id",
            "sha1"
        ]
    },
    "tests": [
        {
            "id": "level0_folder_create",
            "name": "Level 0: Create Folder Call",
            "prompt": "Call `box_folder_create_tool` to create a folder named 'Test_Folder_0' with parent_folder_id '0'.",
            "type": "actionEval",
            "seed_template": "box_default",
            "impersonate_user_id": "27512847635",
            "assertions": [
                {
                    "diff_type": "added",
                    "entity": "box_folders",
                    "where": {
                        "name": {
                            "eq": "Test_Folder_0"
                        }
                    },
                    "expected_count": 1
                }
            ]
        },
        {
            "id": "level1_search_file",
            "name": "Level 1: Search for file",
            "prompt": "Search for files with name containing 'fomcminutes'",
            "type": "actionEval",
            "seed_template": "box_default",
            "impersonate_user_id": "27512847635",
            "assertions": []
        },
        {
            "id": "level1_get_file_info",
            "name": "Level 1: Get file info",
            "prompt": "Get information about the file 'goog-10-q-q2-2025.pdf' (ID 1517335716)",
            "type": "actionEval",
            "seed_template": "box_default",
            "impersonate_user_id": "27512847635",
            "assertions": []
        },
        {
            "id": "level1_create_folder",
            "name": "Level 1: Create folder",
            "prompt": "Create a new folder named 'Analysis_2026' inside the 'investments' folder (ID 2217923455)",
            "type": "actionEval",
            "seed_template": "box_default",
            "impersonate_user_id": "27512847635",
            "assertions": [
                {
                    "diff_type": "added",
                    "entity": "box_folders",
                    "where": {
                        "name": {
                            "eq": "Analysis_2026"
                        },
                        "parent_id": {
                            "eq": "2217923455"
                        }
                    },
                    "expected_count": 1
                }
            ]
        },
        {
            "id": "level1_rename_folder",
            "name": "Level 1: Rename folder",
            "prompt": "Rename the 'macroeconomics' folder (ID 1533390929) to 'Global Economics'",
            "type": "actionEval",
            "seed_template": "box_default",
            "impersonate_user_id": "27512847635",
            "assertions": [
                {
                    "diff_type": "changed",
                    "entity": "box_folders",
                    "where": {
                        "id": {
                            "eq": "1533390929"
                        }
                    },
                    "expected_changes": {
                        "name": {
                            "to": {
                                "eq": "Global Economics"
                            }
                        }
                    }
                }
            ]
        },
        {
            "id": "level1_move_file",
            "name": "Level 1: Move file",
            "prompt": "Move the file 'transport-april-2025-csv.csv' (ID 2087591570) into the 'investments' folder (ID 2217923455)",
            "type": "actionEval",
            "seed_template": "box_default",
            "impersonate_user_id": "27512847635",
            "assertions": [
                {
                    "diff_type": "changed",
                    "entity": "box_files",
                    "where": {
                        "id": {
                            "eq": "2087591570"
                        }
                    },
                    "expected_changes": {
                        "parent_id": {
                            "to": {
                                "eq": "2217923455"
                            }
                        }
                    }
                }
            ]
        },
        {
            "id": "level1_add_comment",
            "name": "Level 1: Add comment",
            "prompt": "Add a comment 'Needs review' to the folder 'investments' (ID 2217923455)",
            "type": "actionEval",
            "seed_template": "box_default",
            "impersonate_user_id": "27512847635",
            "assertions": [
                {
                    "diff_type": "added",
                    "entity": "box_comments",
                    "where": {
                        "message": {
                            "contains": "Needs review"
                        },
                        "item_id": {
                            "eq": "2217923455"
                        }
                    },
                    "expected_count": 1
                }
            ]
        },
        {
            "id": "level1_create_task",
            "name": "Level 1: Create task",
            "prompt": "Create a task 'Verify data' for file 'transport-april-2025-csv.csv' (ID 2087591570)",
            "type": "actionEval",
            "seed_template": "box_default",
            "impersonate_user_id": "27512847635",
            "assertions": [
                {
                    "diff_type": "added",
                    "entity": "box_tasks",
                    "where": {
