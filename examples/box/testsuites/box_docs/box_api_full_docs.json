    "POST /comments": {
        "description": "Adds a comment by the user to a specific file, or as a reply to another comment.",
        "parameters": {
            "body": {
                "item": {
                    "type": "object",
                    "required": true,
                    "description": "The item to comment on"
                },
                "item.type": {
                    "type": "string",
                    "required": true,
                    "description": "Either 'file' or 'comment' (for replies)"
                },
                "item.id": {
                    "type": "string",
                    "required": true,
                    "description": "The ID of the file or parent comment"
                },
                "message": {
                    "type": "string",
                    "required": true,
                    "description": "The text of the comment"
                },
                "tagged_message": {
                    "type": "string",
                    "required": false,
                    "description": "Message with @mentions using @[userid:name] format"
                }
            }
        },
        "example_request": "curl -i -X POST \"https://api.box.com/2.0/comments\" \\\n     -H \"authorization: Bearer <ACCESS_TOKEN>\" \\\n     -H \"content-type: application/json\" \\\n     -d '{\n       \"message\": \"Review completed!\",\n       \"item\": {\n         \"type\": \"file\",\n         \"id\": \"12345\"\n       }\n     }'"
    },
    "POST /tasks": {
        "description": "Creates a single task on a file. This task is not assigned to any user and will need to be assigned separately.",
        "parameters": {
            "body": {
                "item": {
                    "type": "object",
                    "required": true,
                    "description": "The file to create task on"
                },
                "item.type": {
                    "type": "string",
                    "required": true,
                    "description": "Must be 'file'"
                },
                "item.id": {
                    "type": "string",
                    "required": true,
                    "description": "The file ID"
                },
                "action": {
                    "type": "string",
                    "required": false,
                    "description": "Task action: 'review' (default) or 'complete'"
                },
                "message": {
                    "type": "string",
                    "required": false,
                    "description": "Task description"
                },
                "due_at": {
                    "type": "string",
                    "required": false,
                    "description": "Due date (ISO 8601 format)"
                },
                "completion_rule": {
                    "type": "string",
                    "required": false,
                    "description": "'all_assignees' (default) or 'any_assignee'"
                }
            }
        },
        "example_request": "curl -i -X POST \"https://api.box.com/2.0/tasks\" \\\n     -H \"authorization: Bearer <ACCESS_TOKEN>\" \\\n     -H \"content-type: application/json\" \\\n     -d '{\n       \"item\": {\n         \"type\": \"file\",\n         \"id\": \"12345\"\n       },\n       \"action\": \"review\",\n       \"message\": \"Please review this document\"\n     }'"
    },
