{
    "GET /users/me": {
        "description": "Retrieves information about the user who is currently authenticated.",
        "parameters": {
            "query": {
                "fields": {
                    "type": "string",
                    "required": false,
                    "description": "Comma-separated list of fields to include in the response"
                }
            }
        },
        "example_request": "curl -i -X GET \"https://api.box.com/2.0/users/me\" \\\n     -H \"authorization: Bearer <ACCESS_TOKEN>\""
    },
    "GET /search": {
        "description": "Searches for files, folders, and web links.",
        "parameters": {
            "query": {
                "query": {
                    "type": "string",
                    "required": true,
                    "description": "The search term to look for"
                },
                "type": {
                    "type": "string",
                    "required": false,
                    "description": "Filter by type: file, folder, or web_link"
                },
                "file_extensions": {
                    "type": "string",
                    "required": false,
                    "description": "Comma-separated list of file extensions to filter by"
                },
                "ancestor_folder_ids": {
                    "type": "string",
                    "required": false,
                    "description": "Comma-separated folder IDs to limit search scope"
                },
                "content_types": {
                    "type": "string",
                    "required": false,
                    "description": "Filter by content type: name, description, file_content, comments, tag"
                },
                "limit": {
                    "type": "integer",
                    "required": false,
                    "description": "Maximum number of results to return (default: 30, max: 200)"
                },
                "offset": {
                    "type": "integer",
                    "required": false,
                    "description": "Pagination offset"
                }
            }
        },
        "example_request": "curl -i -X GET \"https://api.box.com/2.0/search?query=report\" \\\n     -H \"authorization: Bearer <ACCESS_TOKEN>\""
    },
    "POST /folders": {
        "description": "Creates a new empty folder within the specified parent folder.",
        "parameters": {
            "body": {
                "name": {
                    "type": "string",
                    "required": true,
                    "description": "The name for the new folder"
                },
                "parent": {
                    "type": "object",
                    "required": true,
                    "description": "The parent folder object"
                },
                "parent.id": {
                    "type": "string",
                    "required": true,
                    "description": "The ID of the parent folder (use '0' for root)"
                }
            }
        },
        "example_request": "curl -i -X POST \"https://api.box.com/2.0/folders\" \\\n     -H \"authorization: Bearer <ACCESS_TOKEN>\" \\\n     -H \"content-type: application/json\" \\\n     -d '{\n       \"name\": \"New Folder\",\n       \"parent\": {\n         \"id\": \"0\"\n       }\n     }'"
    },
    "GET /folders/{folder_id}": {
        "description": "Retrieves details for a folder, including the first 100 entries in the folder.",
        "parameters": {
            "path": {
                "folder_id": {
                    "type": "string",
                    "required": true,
                    "description": "The unique identifier of the folder. Use '0' for root folder."
                }
            },
            "query": {
                "fields": {
                    "type": "string",
                    "required": false,
                    "description": "Comma-separated list of fields to include"
                },
                "sort": {
                    "type": "string",
                    "required": false,
                    "description": "Sort by: id, name, or date"
                },
                "direction": {
                    "type": "string",
                    "required": false,
                    "description": "Sort direction: ASC or DESC"
                },
                "offset": {
                    "type": "integer",
                    "required": false,
                    "description": "Pagination offset"
                },
                "limit": {
                    "type": "integer",
                    "required": false,
                    "description": "Maximum items to return (max: 1000)"
                }
            }
        },
        "example_request": "curl -i -X GET \"https://api.box.com/2.0/folders/12345\" \\\n     -H \"authorization: Bearer <ACCESS_TOKEN>\""
    },
    "PUT /folders/{folder_id}": {
        "description": "Updates a folder. Can be used to rename or move a folder, or to add it to a collection.",
        "parameters": {
            "path": {
                "folder_id": {
                    "type": "string",
                    "required": true,
                    "description": "The unique identifier of the folder"
                }
            },
            "header": {
                "If-Match": {
                    "type": "string",
                    "required": false,
                    "description": "Conditional update - fails with 412 if etag doesn't match"
                }
            },
            "body": {
                "name": {
                    "type": "string",
                    "required": false,
                    "description": "New name for the folder"
                },
                "description": {
                    "type": "string",
                    "required": false,
                    "description": "New description"
                },
                "parent": {
                    "type": "object",
                    "required": false,
                    "description": "{\"id\": \"new_parent_id\"} to move the folder"
                },
                "tags": {
                    "type": "array",
                    "required": false,
                    "description": "Array of tag strings"
                },
                "collections": {
                    "type": "array",
                    "required": false,
                    "description": "Array of collection objects to add/remove folder from"
                }
            }
        },
        "example_request": "curl -i -X PUT \"https://api.box.com/2.0/folders/12345\" \\\n     -H \"authorization: Bearer <ACCESS_TOKEN>\" \\\n     -H \"content-type: application/json\" \\\n     -d '{\n       \"name\": \"New folder name\"\n     }'"
    },
    "DELETE /folders/{folder_id}": {
        "description": "Deletes a folder, either permanently or by moving it to the trash.",
        "parameters": {
            "path": {
                "folder_id": {
                    "type": "string",
                    "required": true,
                    "description": "The unique identifier of the folder"
                }
            },
            "query": {
                "recursive": {
                    "type": "boolean",
                    "required": false,
                    "description": "If true, delete a non-empty folder recursively"
                }
            },
            "header": {
                "If-Match": {
                    "type": "string",
                    "required": false,
                    "description": "ETag value for precondition check"
                }
            }
        },
        "example_request": "curl -i -X DELETE \"https://api.box.com/2.0/folders/12345\" \\\n     -H \"authorization: Bearer <ACCESS_TOKEN>\""
    },
    "GET /folders/{folder_id}/items": {
        "description": "Retrieves a page of items in a folder. These items can be files, folders, and web links.",
        "parameters": {
            "path": {
                "folder_id": {
                    "type": "string",
                    "required": true,
                    "description": "The folder ID. Use '0' for root folder."
                }
            },
            "query": {
                "fields": {
                    "type": "string",
                    "required": false,
                    "description": "Comma-separated list of fields"
                },
                "limit": {
                    "type": "integer",
                    "required": false,
                    "description": "Max number of items (default: 100, max: 1000)"
                },
                "offset": {
                    "type": "integer",
                    "required": false,
                    "description": "Offset for pagination"
                },
                "sort": {
                    "type": "string",
                    "required": false,
                    "description": "Sort by: id, name, or date"
                },
                "direction": {
                    "type": "string",
                    "required": false,
                    "description": "Sort direction: ASC or DESC"
                }
            }
        },
        "example_request": "curl -i -X GET \"https://api.box.com/2.0/folders/12345/items\" \\\n     -H \"authorization: Bearer <ACCESS_TOKEN>\""
    },
    "POST /files/content": {
        "description": "Uploads a small file to Box. For file sizes over 50MB use the Chunk Upload APIs.",
        "parameters": {
            "body": {
                "attributes": {
                    "type": "JSON",
                    "required": true,
                    "description": "File attributes including name and parent. Must come BEFORE the file part."
                },
                "attributes.name": {
                    "type": "string",
                    "required": true,
                    "description": "The name of the file"
                },
                "attributes.parent.id": {
                    "type": "string",
                    "required": true,
                    "description": "The ID of the parent folder"
                },
                "file": {
                    "type": "binary",
                    "required": true,
                    "description": "The binary file content"
                }
            }
        },
        "example_request": "curl -i -X POST \"https://upload.box.com/api/2.0/files/content\" \\\n     -H \"authorization: Bearer <ACCESS_TOKEN>\" \\\n     -H \"content-type: multipart/form-data\" \\\n     -F attributes='{\"name\":\"Contract.pdf\", \"parent\":{\"id\":\"11446498\"}}' \\\n     -F file=@<FILE_NAME>"
    },
    "GET /files/{file_id}": {
        "description": "Retrieves the details about a file.",
