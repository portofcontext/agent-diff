      "prompt": "Create a new issue in the Engineering team titled 'Fix login bug' with high priority",
      "type": "actionEval",
      "seed_template": "linear_default",
      "impersonate_user_id": "2790a7ee-fde0-4537-9588-e233aa5a68d1",
      "assertions": [
        {
          "diff_type": "added",
          "entity": "issues",
          "where": {
            "teamId": {
              "eq": "ad608998-915c-4bad-bcd9-85ebfccccee8"
